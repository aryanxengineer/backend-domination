<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>

    <% images.forEach((image) => { %>

    <img
      src="data:image/png;base64,<%= image.buffer.toString('base64') %>"
      alt="Uploaded Image"
    />

    <% }) %>

  </body>
</html>
